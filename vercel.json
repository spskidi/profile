{
  "version": 2,
  "builds": [
    {
      "src": "portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb", "runtime": "python3.11" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static"
      }
    }
  ],

  "routes": [
    
    {
      "src": "/(.*)",
      "dest": "portfolio/wsgi.py"
    }
  ],
  "outputDirectory": "static"
}
