{
  "version": 2,
  "builds": [
    {
      "src": "profileportfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" ,
         "runtime": "python3.11"
        }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles_build"
           
        }
    }
  ],
  "buildCommand": "bash build_files.sh",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "profileportfolio/wsgi.py"
    }
  ]
}